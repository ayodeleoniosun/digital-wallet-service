{
  "name": "wallet-service",
  "version": "1.0.0",
  "description": "A RESTful API to create, fund, transfer to digital wallets",
  "scripts": {
    "start": "node dist/server.js",
    "clean": "rimraf dist/*",
    "postinstall": "tsc",
    "build": "npm run clean && npm run postinstall",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test:dev": "jest --watch",
    "test": "set NODE_ENV=test && jest --config jest.config.js --runInBand --verbose --coverage --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayodeleoniosun/digital-wallet-service"
  },
  "author": "Ayodele Oniosun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ayodeleoniosun/digital-wallet-service/issues"
  },
  "homepage": "https://github.com/ayodeleoniosun/digital-wallet-service#readme",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "emailjs": "^4.0.3",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.0",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "sequelize": "^6.37.3",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.6.2",
    "typedi": "^0.10.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.6",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.6.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
